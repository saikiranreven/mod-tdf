{
    "steps": [
      {
        "name": "gcr.io/cloud-builders/gcloud",
        "args": [
          "run",
          "deploy",
          "live-clock",
          "--source=.",
          "--region=us-central1",
          "--platform=managed",
          "--allow-unauthenticated",
          "--set-env-vars=TZ=UTC",
          "--port=8080"
        ]
      }
    ],
    "timeout": "1100s",
    "options": {
      "logging": "CLOUD_LOGGING_ONLY",
      "machineType": "E2_HIGHCPU_8"
    }
  }